type Route {
  id: ID! @id
  createdAt: DateTime! @createdAt
  title: String!
  points: Int!
  attempts: Int!
  climbedBy: User
}

type User {
  id: ID! @id
  firstName: String!
  lastName: String!
  email: String! @unique
  password: String!
  routes: [Route!]!
}

type Signature {
  id: ID! @id
  author: User!
  route: Route!
}
